// Zed settings
//
// For information on how to configure Zed, see the Zed
// documentation: https://zed.dev/docs/configuring-zed
//
// To see all of Zed's default settings without changing your
// custom settings, run `zed: open default settings` from the
// command palette
{
  "features": {
    "inline_completion_provider": "none"
  },
  "assistant": {
    "default_model": {
      "provider": "google",
      "model": "gemini-1.5-flash"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "project_panel": {
    "dock": "right",
    "default_width": 200
  },
  "outline_panel": {
    "dock": "right",
    "default_width": 200
  },
  "chat_panel": {
    "dock": "right",
    "default_width": 200
  },
  "collaboration_panel": {
    "dock": "right",
    "default_width": 200
  },
  "theme": {
    "mode": "dark",
    "light": "Catppuccin Mocha",
    "dark": "Catppuccin Mocha"
  }
}
